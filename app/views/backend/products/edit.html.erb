<div class ='row'>
	<div class ='col-lg-12'>
		<h2>แก้ไขที่ดิน <%=@product.title%></h2>
	</div>
</div>
<div>
	<%=form_for [:backend, @product] do |f|%>
		  
 		  
		  <%=render :partial => "form", :locals => {:f => f}%>
		  <div class="form-group">
		  		<%=f.submit "บันทึก", {:class => "btn btn-success"}%>
		  		<%=link_to "ย้อนกลับ", backend_products_path, {:class => "btn btn-default"}%>
		  </div>
	<%end%>
</div>